		short bigShort = 32767;
		//int bigInt = //21 billion;
		long b = 55L;
		float bigFloat = 3.14F;
		double bigDouble = 3.123178975308538686;
		
		System.out.println(Float.MAX_VALUE);
		System.out.println(Double.MAX_VALUE);
		
		boolean trueOrFalse = true;
		
		char randomChar = 65;
		char anotherChar = 'A'; //Cannot be double quotes
		
		char escapedChars = '\f';
		
		String randomString = "I'm a random String";
		String anotherString = "stuff";
		String andAnotherString = randomString + " " + anotherString;
		
		String ByteString = Byte.toString(bigByte);
